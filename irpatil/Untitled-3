    <script type="text/javascript">
      function OpenNewWindow() {
        window.open(
          (toolbar = no),
          (status = no),
          (menubar = no),
          (location = center),
          (scrollbars = no),
          (resizable = no),
          (height = 500),
          (width = 657)
        );
      }
